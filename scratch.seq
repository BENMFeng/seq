import math

NAN = math.nan
INF = math.inf
NINF = -INF

print math.gamma(6.0) == 120.0
print math.gamma(1.0) == 1.0
print math.gamma(2.0) == 1.0
print math.gamma(3.0) == 2.0
print math.isnan(math.gamma(-1.0)) == True
print math.gamma(INF) == INF
print math.isnan(math.gamma(NINF)) == True
print math.isnan(math.gamma(NAN)) == True
