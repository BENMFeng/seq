import math

NAN = math.nan
INF = math.inf
NINF = -INF

print math.atanh(0.0) == 0
print math.atanh(0.5) == 0.5493061443340549
print math.atanh(-0.5) == -0.5493061443340549
print math.isnan(math.atanh(INF)) == True
print math.isnan(math.atanh(NAN)) == True
print math.isnan(math.atanh(NINF)) == True
