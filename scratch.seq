import math

NAN = math.nan
INF = math.inf
NINF = -INF

print math.tan(0.0) == 0
print math.tan(math.pi/4) == 0.9999999999999999
print math.tan(-math.pi/4) == -0.9999999999999999
print math.isnan(math.tan(INF)) == True
print math.isnan(math.tan(NINF)) == True
print math.isnan(math.tan(NAN)) == True
