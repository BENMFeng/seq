import math

NAN = math.nan
INF = math.inf
NINF = -INF

print math.log2(1.0) == 0.0
print math.log2(2.0) == 1.0
print math.log2(4.0) == 2.0
print math.log2(2.0**1023) == 1023.0
print math.isnan(math.log2(-1.5)) == True
print math.isnan(math.log2(NINF)) == True
print math.isnan(math.log2(NAN)) == True
