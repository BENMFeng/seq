import math

NAN = math.nan
INF = math.inf
NINF = -INF

print math.cos(0.0) == 1
print math.cos(math.pi/2) == 6.123233995736766e-17
print math.cos(-math.pi/2) == 6.123233995736766e-17
print math.cos(math.pi) == -1
print math.isnan(math.cos(INF)) == True
print math.isnan(math.cos(NINF)) == True
print math.isnan(math.cos(NAN)) == True
