import math

NAN = math.nan
INF = math.inf
NINF = -INF

print math.trunc(1.0) == 1
print math.trunc(-1.0) == -1
print math.trunc(1.5) == 1
print math.trunc(-1.5) == -1
print math.trunc(1.99999999) == 1
print math.trunc(-1.99999999) == -1
print math.trunc(0.99999999) == 0
print math.trunc(-100.999) == -100
