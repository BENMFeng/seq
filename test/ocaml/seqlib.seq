# Seq library 

extend int:
  def iter(self):
    i = 0
    while i < self:
      yield i
      i += 1

extend file:
  def iter(self): # -> yieldable[seq]:
    while True:
      n = self.read()
      if n == 0:
        break
      for i in n.iter():
        yield self.get(i)
    self.close()

